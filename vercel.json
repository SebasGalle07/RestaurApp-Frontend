{
  "framework": "angular",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/restaurapp-frontend/browser",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://restaurapp-backend.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
